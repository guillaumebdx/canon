{
  "expo": {
    "name": "Brick Shot Galaxy",
    "slug": "brick-shot-galaxy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guillaumebdx.brickshotgalaxy",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0a0a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.guillaumebdx.brickshotgalaxy"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-audio",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "c5eb6125-f53b-4819-8404-3b6b1a500011"
      }
    },
    "owner": "guillaumebdx"
  }
}
